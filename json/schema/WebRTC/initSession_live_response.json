{
    "$id": "initSession_live_response",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Complete \"live\" initSession response from server",
    "description": "This message is sent from signaling server to client in response to an initSession request from the client.\nThe `data` field contains the exact same data that was sent in the request message.",
    "type": "object",
    "properties": {
        "type": {
            "type": "string",
            "const": "initSession",
            "description": "Specifies an initSession request"
        },
        "targetId": {
            "type": "string",
            "examples": [
                "mycamera"
            ],
            "description": "The target id to which the client want to establish the WebRTC session"
        },
        "orgId": {
            "type": "string",
            "examples": [
                "myorg"
            ],
            "description": "The id of the organization that the device belongs to."
        },
        "correlationId": {
            "type": "string",
            "description": "Contains the same data that was sent in the request `correlationId` field."
        },
        "turnServers": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "urls": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "examples": [
                            [
                                "turns:sw30u5726ml0mog.turn.dev.webrtc.connect.axis.com:443?transport=tcp"
                            ],
                            [
                                "turns:sw30u5726ml0mog.turn.dev.webrtc.connect.axis.com:443?transport=udp"
                            ]
                        ]
                    },
                    "username": {
                        "type": "string",
                        "examples": [
                            "<username>"
                        ]
                    },
                    "password": {
                        "type": "string",
                        "examples": [
                            "<password>"
                        ]
                    }
                }
            }
        },
        "stunServers": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "urls": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "examples": [
                            [
                                "stun:sw30u5726ml0mog.turn.dev.webrtc.connect.axis.com:5349"
                            ]
                        ]
                    }
                }
            }
        },
        "data": {
            "$ref": "#/definitions/P2P"
        }
    },
    "required": [
        "type",
        "targetId",
        "orgId",
        "data"
    ],
    "definitions": {
        "P2P": {
            "$id": "initSession_request_live",
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "title": "initSession request for live media",
            "description": "This message is sent from client to device to request a new WebRTC session.\nMedia properties of the session are determined by the `profile`.",
            "type": "object",
            "properties": {
                "apiVersion": {
                    "type": "string",
                    "const": "1.0",
                    "pattern": "^[0-9]+\\.[0-9]+$",
                    "description": "The API version that should be used in the request."
                },
                "type": {
                    "type": "string",
                    "examples": [
                        "request"
                    ],
                    "description": "Specifies that this message is a request"
                },
                "sessionId": {
                    "type": "string",
                    "examples": [
                        "948dd272-f56b-416f-b349-fa1e88578b7c"
                    ],
                    "description": "An identifier chosen by the client that should be used for all subsequent messages belonging to this session."
                },
                "method": {
                    "type": "string",
                    "const": "initSession",
                    "description": "Specifies an initSession request."
                },
                "context": {
                    "type": "string",
                    "examples": [
                        "ee4b07f4-1890-41db-b368-66b246c54fb3"
                    ],
                    "description": "An optional identifier chosen by the client that can be used to correlate the response from the device with the request."
                },
                "params": {
                    "type": "object",
                    "description": "The requested session configuration.",
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "live",
                            "description": "Specifies a live session."
                        },
                        "profile": {
                            "type": "string",
                            "examples": [
                                "my_profile"
                            ],
                            "description": "The media profile to use for streaming."
                        }
                    },
                    "required": [
                        "type"
                    ]
                }
            },
            "required": [
                "apiVersion",
                "sessionId",
                "method",
                "params"
            ]
        }
    }
}